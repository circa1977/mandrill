  "type": "craft-plugin",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "mandrill",
    "mailer"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Pixel & Tonic",
      "homepage": "https://pixelandtonic.com/"
    }
  ],
  "support": {
    "email": "support@craftcms.com",
    "issues": "https://github.com/craftcms/mandrill/issues?state=open",
    "source": "https://github.com/craftcms/mandrill",
